    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.2.3",
    "redis": "^4.6.11",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-elasticsearch": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/node": "^20.10.5",
    "artillery": "^2.0.23",
    "babel-jest": "^30.0.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "html-pdf-node": "^1.0.7",
    "jest": "^29.7.0",
    "markdown-pdf": "^11.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^6.12.0",
    "puppeteer": "^24.14.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Moses-Githinji/kenya-votes-live-BACKEND.git"
  },
  "bugs": {
    "url": "https://github.com/Moses-Githinji/kenya-votes-live-BACKEND.git/issues"
  },
  "homepage": "https://github.com/Moses-Githinji/kenya-votes-live-BACKEND.git#readme"
}
