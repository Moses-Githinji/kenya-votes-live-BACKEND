{
  "name": "kenya-votes-live-backend",
  "version": "1.0.0",
  "description": "Backend API for Kenya Votes Live - Real-time election monitoring system",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "npm run build:prisma && npm run build:app",
    "build:prisma": "prisma generate",
    "build:app": "echo 'App built successfully'",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/middleware/ tests/routes/ --verbose",
    "test:integration": "jest tests/integration/ --verbose",
    "test:websocket": "jest tests/websocket/ --verbose",
    "test:auth": "jest tests/middleware/auth.test.js --verbose",
    "test:public": "jest tests/routes/public.test.js --verbose",
    "test:commissioner": "jest tests/routes/commissioner.test.js --verbose",
    "test:returning-officer": "jest tests/routes/returningOfficer.test.js --verbose",
    "test:presiding-officer": "jest tests/routes/presidingOfficer.test.js --verbose",
    "test:election-clerk": "jest tests/routes/electionClerk.test.js --verbose",
    "test:system-admin": "jest tests/routes/systemAdmin.test.js --verbose",
    "test:all": "node tests/run-tests.js",
    "test:coverage": "jest --coverage --coverageDirectory=coverage",
    "test:load": "artillery run load-tests/artillery-config.yml",
    "test:extreme-stress": "artillery run load-tests/extreme-stress-test.yml",
    "test:db-stress": "node load-tests/database-stress-test.js",
    "test:million-user-db": "node load-tests/million-user-db-test.js",
    "test:massive-stress": "artillery run load-tests/massive-stress-test.yml",
    "test:performance": "node load-tests/performance-monitor.js",
    "test:full": "npm run test:db-stress && npm run test:load",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "docker:build": "docker build -t kenya-votes-backend .",
    "docker:run": "docker run -p 3000:3000 kenya-votes-backend"
  },
  "keywords": [
    "election",
    "kenya",
    "real-time",
    "api",
    "monitoring",
    "votes",
    "iebc"
  ],
  "author": "Kenya Votes Live Team",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.2",
    "@aws-sdk/client-s3": "^3.848.0",
    "@aws-sdk/s3-request-presigner": "^3.848.0",
    "@prisma/client": "^6.12.0",
    "aws-sdk": "^2.1490.0",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.0.0",
    "csv-stringify": "^6.6.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "elasticsearch": "^16.7.3",
    "express": "^4.18.2",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-http-proxy": "^1.6.3",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-status-monitor": "^1.2.3",
    "express-validator": "^7.0.1",
    "express-ws": "^5.0.2",
